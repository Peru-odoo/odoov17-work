<odoo>

    <record id="ir_cron_send_telegram_english" model="ir.cron">
        <field name="name">Send Telegram, Study English</field>
        <field name="model_id" ref="english_learning.model_english_dictionary"/>
        <field name="state">code</field>
        <field name="code">model.send_telegram()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>

</odoo>